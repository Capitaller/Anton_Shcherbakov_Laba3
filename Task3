ng System;
using System.Linq;

namespace LabaProga3._3
{
    class Program
    {
        static void Main(string[] args)
        {

            int[] numbers = { 3, 12,1, 10, 34,4, 20, 55, 66, 77, 88 };
         
           var result = from t in numbers where t%2 > 0 orderby t select t;
                 foreach (int t in result)
                
                    Console.WriteLine(t);
        
        }
    }
}
